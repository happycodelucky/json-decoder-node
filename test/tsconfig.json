{
    "extends": "../tsconfig.json",
    "compilerOptions": {
        "experimentalDecorators": true,
        "noImplicitAny": false,
        "noImplicitThis": false,
        "noEmit": true,
        "noEmitOnError": false,
        "noImplicitReturns": false,
        "noUnusedLocals": false,
        "allowJs": true
    },
    "include": ["**/*.ts"]
}